<wxs module="fun">
function rgb(a){//rgb颜色随机
	if(!a){
		a=0.5;
	}
	var r = Math.floor(Math.random()*256);
	var g = Math.floor(Math.random()*256);
	var b = Math.floor(Math.random()*256);
	var rgb = 'rgba('+r+','+g+','+b+','+a+')';
	return rgb;
}
module.exports = {
	rgb:rgb
}
</wxs>
